
@{
    ViewBag.Title = "Awards";
}

<div class="reward-body">

    <h2>Samsung Galaxy Tab S2</h2>

    <div id="myCarousel" class="carousel slide reward-carousel" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
            <li data-target="#myCarousel" data-slide-to="3"></li>
            <li data-target="#myCarousel" data-slide-to="4"></li>
            <li data-target="#myCarousel" data-slide-to="5"></li>
            <li data-target="#myCarousel" data-slide-to="6"></li>
            <li data-target="#myCarousel" data-slide-to="7"></li>
            <li data-target="#myCarousel" data-slide-to="8"></li>
            <li data-target="#myCarousel" data-slide-to="9"></li>
            <li data-target="#myCarousel" data-slide-to="10"></li>
            <li data-target="#myCarousel" data-slide-to="11"></li>
            <li data-target="#myCarousel" data-slide-to="12"></li>
            <li data-target="#myCarousel" data-slide-to="13"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img class="reward-img" src="~/Content/Images/Reward/01-Front.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/02-Back.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/03-Perspective.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/04-Perspective.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/05-R-Side.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/06-L-Side.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/07-Dynamic.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/08-Dynamic.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/09-Dynamic.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/10-Detail.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/11-Detail.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/12-Detail.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/13-Detail.jpg" alt="Galaxy Tab S2">
            </div>

            <div class="item">
                <img class="reward-img" src="~/Content/Images/Reward/14-Combination.jpg" alt="Galaxy Tab S2">
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

    <table class="rewards-more-info">
        <tr>
            <th>
                <h3>Допълнителна информация</h3>
            </th>
        </tr>
        <tr>
            <td>Процесор</td>
            <td>1.9GHz, 1.3GHz</td>
            <td>Octa-Core</td>
        </tr>
        <tr>
            <td>Дисплей</td>
            <td>9.7"(245.8mm)</td>
            <td>Super AMOLED 2048x1536(QXGA)</td>
        </tr>
        <tr>
            <td>Камера</td>
            <td>8 мегапиксела</td>
            <td></td>
        </tr>
        <tr>
            <td>Памет</td>
            <td>3GB Оперативна памет</td>
            <td>32GB Вградена памет</td>
        </tr>
        <tr>
            <td>Батерия</td>
            <td>Li-Ion 5870 mAh</td>
            <td></td>
        </tr>
        <tr>
            <td>Операционна система</td>
            <td>Android OS v5.0.2</td>
            <td></td>
        </tr>
    </table>

</div>
