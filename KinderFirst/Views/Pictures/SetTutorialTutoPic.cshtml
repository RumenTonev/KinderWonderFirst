@model List<KinderFirst.Models.TutoPicture>
@{
    ViewBag.Title = "SetTutorialTutoPic";
}
 <div class="row">
    @foreach (var item in Model)
    {

        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 left tutorial-selection">
            <img class="img-responsive" src="@Url.Content(String.Format("~/Content/Images/{0}{1}.jpg",item.TutoId,item.Number))" />
        </div>
    }
</div>
     
